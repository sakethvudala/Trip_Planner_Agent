# Google Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# Gemini Model Configuration
GEMINI_MODEL_NAME=gemini-1.5-flash

# Application Settings
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
APP_ENV=local

# Server Configuration
HOST=0.0.0.0
PORT=8000
BASE_URL=http://localhost:8000
API_PREFIX=/api

# CORS (comma-separated if multiple)
ALLOWED_ORIGINS=*

# Agent Configuration
AGENT_NAME=Trip Planner Agent
AGENT_DESCRIPTION=A multi-agent system for intelligent trip planning and management.
AGENT_VERSION=1.0.0
AGENT_SUPPORT_EMAIL=support@trip-planner.com
AGENT_SUPPORT_URL=https://trip-planner.com/support
AGENT_LOGO_URL=${BASE_URL}/static/logo.png
AGENT_LEGAL_URL=https://trip-planner.com/legal
